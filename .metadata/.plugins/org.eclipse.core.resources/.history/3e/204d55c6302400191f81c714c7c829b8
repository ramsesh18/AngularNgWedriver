package elements;


import org.openqa.selenium.WebDriver;
import org.openqa.selenium.WebElement;

import com.paulhammant.ngwebdriver.ByAngular;

public class FindElementIdentifierUsingNgRepeat  {
	
	WebDriver driver;

	public FindElementIdentifierUsingNgRepeat(WebDriver driver) {
		this.driver = driver;
	}

	protected WebElement findElementUsingBinding(String bindingIdentifier){
		return driver.findElement(ByAngular.binding(bindingIdentifier));
	}
	

}
